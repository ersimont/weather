{
  "name": "weather",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "yarn trim-moment-locales",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --quiet",
    "deploy": "ng deploy",
    "prepwa-dev-server": "ng build",
    "pwa-dev-server": "http-server -p 8080 -c-1 dist/weather",
    "prettier-all": "prettier --write \"**/*.{css,html,js,json,md,scss,ts,yml}\"",
    "predeploy-staging": "ng build",
    "deploy-staging": "firebase hosting:channel:deploy staging",
    "source-map-explorer": "ng build --source-map && source-map-explorer 'dist/weather/main.*.js'",
    "trim-moment-locales": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" src/scripts/trim-moment-locales.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "13.0.2",
    "@angular/common": "~13.0.0",
