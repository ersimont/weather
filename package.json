{
  "name": "weather",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "yarn trim-moment-locales",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && eslint . --ext .js,.jsx,.ts,.tsx",
    "e2e": "ng e2e",
    "web-worker-start": "http-server -p 8080 -c-1 dist/weather",
    "prettier-all": "prettier --write \"**/*.{css,html,js,json,md,scss,ts,yml}\"",
    "deploy": "ng build --prod --output-path docs --base-href /weather/ && git add docs",
    "source-map-explorer": "ng build --prod --sourceMap && source-map-explorer 'dist/weather/main-es2015.*.js'",
    "trim-moment-locales": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" src/scripts/trim-moment-locales.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/cdk": "^9.2.2",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@angular/service-worker": "~9.1.4",
    "@s-libs/app-state": "^0.0.7",
    "@s-libs/js-core": "^0.0.7",
    "@s-libs/micro-dash": "^0.0.7",
    "@s-libs/ng-app-state": "^0.0.7",
    "@s-libs/ng-core": "^0.0.7",
    "@s-libs/rxjs-core": "^0.0.7",
    "@types/suncalc": "^1.8.0",
    "chart.js": "^2.8.0",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-zoom": "^0.7.7",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.29",
    "rxjs": "~6.5.4",
    "suncalc": "^1.8.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@s-libs/ng-dev": "^0.0.7",
    "@types/chart.js": "^2.9.20",
    "@types/chartjs-plugin-annotation": "^0.5.0",
    "@types/google.analytics": "^0.0.40",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "http-server": "^0.12.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.0.2",
    "protractor": "~5.4.3",
    "source-map-explorer": "^2.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "utility-types": "^3.10.0"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  }
